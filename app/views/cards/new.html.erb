<%= render "shared/card", locals: {card: @card} %>
<%= form_for @card,url: {action:"create",method: :post} do |f| %>
    <div>
      <%= f.label :front, 'Front' %>
      <%= f.text_field :front %>
    </div>
    <div>
      <%= f.label :back, 'Back' %>
      <%= f.text_field :back %>
    </div>
    <%= f.submit 'New Card' %>
<% end %>